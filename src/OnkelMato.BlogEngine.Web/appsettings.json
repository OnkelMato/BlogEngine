{
  "ConnectionStrings": {
    "DefaultProvider": "sqlite",
    "DefaultConnection": "Data Source=/data/blog.db;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Posts": {
    //"BlogUniqueId": "00000000-0000-0000-0000-000000000000",
    "UseSingleBlog": true,
    "CreateBlogIfNotExist": true,
    "PageSize": 12,
    "AllowBlogAdministration": true,
    "AcceptUnsignedImport": true,
    "CertificateFile": "/data/import.crt"
  }
}
